<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.visuotech.matesspot">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Act_DashBoard"></activity>
        <activity android:name=".Act_Chat" />
        <activity android:name=".Act_Navigation_Home" />
        <activity android:name=".Act_Signup" />
        <activity android:name=".Act_Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Act_Home" />
        <activity android:name=".Act_VerifyPhone" />
        <activity android:name=".Act_Otpmobile" />
    </application>

</manifest>